{
    "$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Cart",
	"type": "object",
	"typeName": "Cart",
	"properties": {
		"id": {
			"description": "The unique identifier for a cart",
			"type": "number"
		},
        "products":{
            "type": "array",
			"minItems": 0,
			"maxItems": 100,
            "items": {
                "type": "number"
                }
        }
	},
	"required": ["id", "products"]
}